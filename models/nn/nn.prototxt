name: "nn_simple"

layer {
    name: "data"
    type: "Input"
    top: "data"
    input_param {
        shape: {
            dim: 10000
            dim: 34
        }
    }
}

layer {
    name: "target"
    type: "Input"
    top: "target"
    include {
        phase: TRAIN
    }
    input_param {
        shape: {
            dim: 10000
            dim: 1
        }
    }
}

layer {
    name: "ip1"
    type: "InnerProduct"
    top: "ip1"
    bottom: "data"

    inner_product_param {
        num_output: 256
        weight_filler {
            type: "xavier"
            variance_norm: AVERAGE
        }
        bias_filler {
            type: "constant"
        }
    }
}

layer {
    name: "do1"
    type: "Dropout"
    top: "do1"
    bottom: "ip1"
    dropout_param {
        dropout_ratio: 0.1
    }
}

layer {
    name: "ip2"
    type: "InnerProduct"
    top: "ip2"
    bottom: "do1"

    inner_product_param {
        num_output: 256
        weight_filler {
            type: "xavier"
            variance_norm: AVERAGE
        }
        bias_filler {
            type: "constant"
        }
    }
}

layer {
    name: "do2"
    type: "Dropout"
    top: "do2"
    bottom: "ip2"
    dropout_param {
        dropout_ratio: 0.1
    }
}

layer {
    name: "output"
    type: "InnerProduct"
    top: "output"
    bottom: "do2"

    inner_product_param {
        num_output: 1
        weight_filler {
            type: "xavier"
            variance_norm: AVERAGE
        }
        bias_filler {
            type: "constant"
        }
    }
}

layer {
    name: "loss"
    type: "EuclideanLoss"
    bottom: "output"
    bottom: "target"
    top: "loss"
    include {
        phase: TRAIN
    }
}
